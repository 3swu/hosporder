      <script>
          $(document).ready(function () {
              $("#button").click(function () {
                  confirm();
              });
          });

          function confirm(){
              var customerName = $("#name").val();
              var customerMobile = $("#verify_phone_num").val();
              var dateId = sessionStorage.dateId;
              if(customerName == "" || customerMobile == ""){
                  alert("请输入完整的就诊人信息！");
              }
              else{
                  var customerid;
                  $.ajax({
                      //url:'http://localhost:8080/customer/customer',
                      url:'http://wulei.kim:8080/hosporder_snapshot_v1/customer/customer',
                      type:'POST',
                      contentType:'application/json;charset=utf-8',
                      data:JSON.stringify({"name":customerName,"mobile":parseInt(customerMobile)}),
                      success:function(data){
                          resultObj = JSON.parse(data);
                          var code = resultObj['code'];
                          if(code == 0){
                              alert("添加就诊人失败！");
                          }
                          else{
                              customerid = code;
                              $.ajax({
                                  //url:'http://localhost:8080/customer/order',
                                  url:'http://wulei.kim:8080/hosporder_snapshot_v1/customer/order',
                                  type:'PUT',
                                  contentType:'application/json;charset=utf-8',
                                  data:JSON.stringify({"customerid":customerid,"dateid":dateId}),
                                  success:function (d) {
                                      reltObj = JSON.parse(d);
                                      var cod = reltObj['code'];
                                      alert(reltObj['msg']);
                                  }
                              })
                          }
                      },
                      error:function (){
                          alert("操作失败！");
                      }
                  })
              }
          }


      </script>


